{
  "name": "angular-rspack-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:rspack": "bun run rspack build && cp -r node_modules/winbox/dist/* dist/angular-rspack-demo/",
    "serve:rspack": "bun run rspack serve",
    "dev": "bun run rspack serve",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "biome check --write",
    "lint:check": "biome check",
    "lint:ci": "biome check --reporter=github",
    "format": "biome format --write",
    "format:check": "biome format",
    "check": "biome check --write",
    "e2e": "playwright test",
    "prepare": "husky || true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "winbox": "^0.2.82",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/language-service": "^21.0.0",
    "@biomejs/biome": "^2.4.4",
    "@playwright/test": "^1.58.2",
    "@rspack/cli": "^1.7.6",
    "@rspack/core": "^1.7.6",
    "@types/jsdom": "^28.0.0",
    "@types/node": "^20.19.34",
    "bun-types": "^1.3.9",
    "css-loader": "^7.1.4",
    "esbuild-loader": "^4.4.2",
    "html-rspack-plugin": "^6.1.7",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.7",
    "style-loader": "^4.0.0",
    "typescript": "~5.9.2"
  }
}
